
    @startuml
    package "Service" {
        [blue-service]
    }

    package "Spec" {
        [type: ClusterIP]
        [selector]
        [ports]
    }

    [blue-service] --> [type: ClusterIP] : spec
    [blue-service] --> [selector] : spec
    [blue-service] --> [ports] : spec

    [ports] --> [protocol: TCP]
    [ports] --> [port: 80]
    [ports] --> [targetPort: 80]
    @enduml
    